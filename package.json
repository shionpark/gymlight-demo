{
  "name": "gymlight",
  "packageManager": "yarn@4.4.0",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "start:gymlight-manager": "yarn workspace gymlight-manager start",
    "dev": "concurrently \"yarn workspace gymlight-design-system storybook\" \"yarn workspace gymlight-manager dev\"",
    "dev:gymlight-manager": "yarn workspace gymlight-manager dev",
    "dev:mock": "yarn workspace gymlight-manager dev:mock",
    "build": "yarn workspace gymlight-design-system publish:npm && yarn workspace gymlight-manager build",
    "build:mock": "yarn workspace gymlight-manager build:demo",
    "build:design-system": "yarn workspace gymlight-design-system publish:npm",
    "build:gymlight-manager": "yarn workspace gymlight-manager build",
    "generate": "node scripts/generate-component.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.0",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.2",
    "polished": "^4.3.1",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recoil-persist": "^5.1.0",
    "ts-pattern": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/preset-env": "^7.23.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/eslint": "^8.56.2",
    "@types/node": "^17.0.8",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@yarnpkg/cli": "^4.4.0",
    "@yarnpkg/core": "^4.1.2",
    "babel-loader": "^9.1.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.3.3",
    "webpack": "^5.88.1"
  },
  "resolutions": {
    "@storybook/core-common/webpack": "^5",
    "@storybook/core-server/webpack": "^5",
    "@storybook/react/webpack": "^5",
    "@testing-library/dom": "^9.0.0"
  }
}
